{
  "name": "@brudite-pvt-ltd/skb-fe-shared",
  "version": "1.0.128",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles/global.css",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles/index.js",
      "types": "./dist/styles/index.d.ts"
    },
    "./assets": {
      "import": "./dist/assets/index.js",
      "types": "./dist/assets/index.d.ts"
    },
    "./scripts": {
      "import": "./dist/scripts/index.js",
      "types": "./dist/scripts/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && npm run copy:styles && npm run copy:assets && npm run copy:scripts",
    "copy:styles": "mkdir -p dist/styles && cp src/styles/*.css dist/styles/ && cp src/styles/index.ts dist/styles/index.ts",
    "copy:assets": "mkdir -p dist/assets && cp -R src/assets/* dist/assets/ && cp src/assets/index.ts dist/assets/index.ts",
    "copy:scripts": "mkdir -p dist/scripts && cp src/scripts/*.js dist/scripts/ && cp src/scripts/index.ts dist/scripts/index.ts"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@uiw/codemirror-theme-bespin": "^4.21.20",
    "@uiw/codemirror-theme-dracula": "^4.21.20",
    "@uiw/codemirror-theme-duotone": "^4.21.20",
    "@uiw/codemirror-theme-github": "^4.21.20",
    "@uiw/codemirror-theme-okaidia": "^4.21.20",
    "@uiw/codemirror-theme-vscode": "^4.21.20",
    "@uiw/codemirror-theme-xcode": "^4.21.20",
    "@uiw/react-codemirror": "^4.21.21",
    "canvas-confetti": "^1.9.3",
    "crypto-js": "^4.1.1",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.41.0",
    "@codemirror/lang-cpp": "^6.0.3",
    "@codemirror/lang-go": "^6.0.1",
    "@codemirror/lang-java": "^6.0.2",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-python": "^6.2.1"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/crypto-js": "^4.1.3",
    "@types/html2canvas": "^0.5.35",
    "@types/jsonwebtoken": "^9.0.6",
    "@uiw/codemirror-theme-bespin": "^4.21.20",
    "@uiw/codemirror-theme-dracula": "^4.21.20",
    "@uiw/codemirror-theme-duotone": "^4.21.20",
    "@uiw/codemirror-theme-github": "^4.21.20",
    "@uiw/codemirror-theme-okaidia": "^4.21.20",
    "@uiw/codemirror-theme-vscode": "^4.21.20",
    "@uiw/codemirror-theme-xcode": "^4.21.20",
    "@uiw/react-codemirror": "^4.21.21",
    "microsoft-cognitiveservices-speech-sdk": "^1.41.0",
    "typescript": "^5"
  }
}
